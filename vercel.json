{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "index.js": {
            "memory": 2048,
            "includeFiles": "db.json"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "index.js"
        }
    ]
}