{
    "version": 2,
    "builds": [
        {

            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "functions": {
        "index.js": {
            "memory": 1024,
            "includeFiles": "db.json"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "index.js"
        }
    ]
}